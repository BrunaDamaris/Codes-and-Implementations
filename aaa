#include <stdio.h>
#define max 100000

int main()
{
	double array[max];
	int i,j,n,intervalo=0,number=0,asteriscos[max];
	scanf("%d",&n);
	for(i=0;i < n;i++)
	{
		scanf("%lf",&array[i]);
	}
	for(i=0;i < 10;i++)
	{
		number=0;
		for(j=0;j < n;j++)
		{
			if((array[j] >= intervalo) && (array[j] <= intervalo+1))
			{
				number++;
			}
		}
		asteriscos[i]=number;
		intervalo++;
	}
	return 0;
}
