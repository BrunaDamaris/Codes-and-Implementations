#include <stdio.h>

int percorrer(int arvore[][2],int no)
{
	if(arvore[no][0] == -1 && arvore[no][1] == -1) 
	{
		return 1;
	}
	else
	{
		percorrer(arvore,arvore[no][0]);
		percorrer(arvore,arvore[no][1]);
		return 0;
	}
}
int main()
{
	int root=0,n,i,j,m=0;
	scanf("%d",&n);
	int arvore[n][2];
	int pais[n];
	for(i=0;i < n;i++)
	{
		for(j=0;j < 2;j++)
		{
			scanf("%d",&arvore[i][j]);
		}
	}
	percorrer(arvore,root);
	/*for(i=0;i < n;i++)
	{
		printf("%d\n",pais[i]);
	}*/
	return 0;
}
